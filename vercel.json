{
  "buildCommand": "rm -rf node_modules && npm install --legacy-peer-deps && vite build",
  "framework": null,
  "installCommand": "npm install --legacy-peer-deps",
  "outputDirectory": "dist",
  "devCommand": "vite",
  "cleanUrls": true,
  "trailingSlash": false,
  "ignoreCommand": "echo 'Skipping build verification'",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}